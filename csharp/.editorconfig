# Root EditorConfig for C# (dotnet format, IDEs)
# https://learn.microsoft.com/en-us/dotnet/fundamentals/code-analysis/configuration-files

root = true

[*]
charset = utf-8
indent_style = space
insert_final_newline = true

[*.{cs,csproj}]
indent_size = 4
trim_trailing_whitespace = true

